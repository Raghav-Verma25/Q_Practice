class Stu
{
    int rollno;
    int arr[];

    public Stu(int rollno, int[] arr) {
        this.rollno = rollno;
        this.arr = arr;
    }
    public void display(){
        System.out.println(rollno);
        for (int res:arr){
            System.out.println(res);
        }
    }
}

public class Topic1 {
    public static void main(String[] args) {
        Stu obj1=new Stu(23,new int[]{23_000,56,78,90});
        System.out.println("Welcome");
        obj1.display();
    }

}
